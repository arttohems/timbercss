{
  "name": "timbercss",
  "author": "unlimit.design",
  "version": "0.1.0",
  "description": "A CSS Framework for Rapid Website Prototyping",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlimitDesign/timbercss.git"
  },
  "license": "MIT",
  "homepage": "https://timbercss.com",
